{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5|^8.0.2",
        "barryvdh/laravel-dompdf": "^0.8.7",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "intervention/image": "^2.5",
        "jenssegers/agent": "^2.6",
        "jeroennoten/laravel-adminlte": "^3.1",
        "justbetter/laravel-pagination-with-havings": "^1.4",
        "keyword-extractor/keyword-extractor": "^1.0",
        "laravel/framework": "^7.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "laravelcollective/html": "^6.1",
        "maatwebsite/excel": "^3.1",
        "ncjoes/office-converter": "^1.0",
        "pbmedia/laravel-ffmpeg": "^6.0",
        "phpoffice/phpword": "^0.17.0",
        "rap2hpoutre/laravel-log-viewer": "^1.7",
        "rguedes/pdfmerger": "^1.0",
        "smalot/pdfparser": "*",
        "spatie/laravel-backup": "^6.16",
        "spatie/pdf-to-text": "^1.4",
        "yajra/laravel-datatables-oracle": "^9.0"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/Helpers/helper.php",
            "vendor/autoload.php"
            
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
